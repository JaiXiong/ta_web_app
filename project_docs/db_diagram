Account
    user (CharField length=20)
    password (CharField length=20)
    role (CharField length=17)
    street_address (CharField length=35 blank=True)
    email_address (EmailField max_length=30 blank=True)
    phone_number (CharField max_length=12 blank=True)

Course
    name (CharField length=20)
    section (CharField length=3)
    days_of_week (CharField length=8)
    start_time (CharField length=5)
    end_time (CharField length=5)
    instructor (ForeignKey(Account, null=True)
    tas (ManyToManyField(Account, blank=True)
    lab (CharField length=3, null=True)
    lab_sections (CharField length=3 null=True)
